# Platform configuration for Node.js deployment
# Consolidated environment variables to avoid duplicates

option_settings:
  aws:elasticbeanstalk:application:environment:
    NODE_ENV: production
    PORT: 8000
    NPM_USE_PRODUCTION: false
  aws:elasticbeanstalk:container:nodejs:
    ProxyServer: nginx
  aws:elasticbeanstalk:command:
    Timeout: 600 